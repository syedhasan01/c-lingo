#include <iostream>
using namespace std;
int main()
{
int a,b;
char op;

 cout<<"enter any two number"<<endl;
 cin>>a;
 cin>>b;
 cout<<"any any operand(+,-,*,/)"<<endl;
 cin>>op;
 switch (op);
     {
 
     case '+':
     cout<<"the sum of two number is"<<(a+b)<<endl;
     break;
     case '-':
     cout<<"the subraction of two number is"<<(a-b)<<endl;
     break;
     case"*":
     cout<<"the mutiplication of two number is"<<(a*b)<<endl;
     break;
     case '/':
     cout<<"the quotient of two number is"<<(a/b)<<endl;
     break;
     default:
    cout<<"wrong operator";
    break;
     }
return 0;
}
